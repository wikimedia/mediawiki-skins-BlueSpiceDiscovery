<div id="footer" class="mx-0" role="contentinfo">
    <div class="inner">
        <ul id="footer-links">
            {{#defaultfooterlinks}}
            <li {{#class}} class="{{.}}"{{/class}}>
                {{{body}}}
            </li>
            {{/defaultfooterlinks}}

            {{#customfooterlinks}}
            {{{.}}}
            {{/customfooterlinks}}
        </ul>
		<ul id="footer-icons">
			{{#icons}}
			<li>
				<a href="{{url}}" target="{{target}}">
					<img src="{{src}}" height="{{height}}" width="{{width}}" alt="{{alt}}">
				</a>
			</li>
			{{/icons}}
		</ul>
    </div>
</div>
